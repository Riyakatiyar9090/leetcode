#include <bits/stdc++.h>
using namespace std;
int removeDuplicates(vector<int> &nums)
{
    int i = 0;
    int j = 1;
    int n = nums.size();
    while (j < n)
    {
        if (nums[i] != nums[j])
        {
            nums[i + 1] = nums[j];
            i++;
            j++;
        }
        else
        {
            j++;
        }
    }
    return i + 1;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int ans = removeDuplicates(arr);
    cout << ans << endl;
    return 0;
}
