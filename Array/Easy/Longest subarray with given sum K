#include <bits/stdc++.h>
using namespace std;
int longestSubarray(vector<int> &arr, int k)
{
    // code here
    long long sum = 0;
    map<long long, int> mpp;
    int maxlen = 0;
    int n = arr.size();
    for (int i = 0; i < n; i++)
    {
        sum += arr[i];
        if (sum == k)
        {
            maxlen = max(maxlen, i + 1);
        }
        long long rem = sum - k;
        if (mpp.find(rem) != mpp.end())
        {
            int len = i - mpp[rem];
            maxlen = max(maxlen, len);
        }
        if (mpp.find(sum) == mpp.end())
        {
            mpp[sum] = i;
        }
    }

    return maxlen;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    // vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    int k;
    cin >> k;
    int ans = longestSubarray(arr1, k);
    cout << ans << endl;

    return 0;
}
