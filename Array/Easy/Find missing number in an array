#include <bits/stdc++.h>
using namespace std;
int missingNum(vector<int> &arr)
{

    int xor1 = 0;
    int xor2 = 0;
    int n = arr.size();
    for (int i = 0; i < n; i++)
    {
        xor1 = xor1 ^ arr[i];
        xor2 = xor2 ^ (i + 1);
    }
    xor2 = xor2 ^ (n + 1);
    int ans = xor1 ^ xor2;
    return ans;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    int ans = missingNum(arr1);
    cout << ans << endl;

    return 0;
}
