#include <bits/stdc++.h>
using namespace std;
int search(vector<int> &arr, int x)
{
    // code here
    for (int i = 0; i < arr.size(); i++)
    {
        if (arr[i] == x)
        {
            return i;
        }
    }
    return -1;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int d;
    cin >> d;
    int ans = search(arr, d);
    cout << ans << endl;

    return 0;
}
