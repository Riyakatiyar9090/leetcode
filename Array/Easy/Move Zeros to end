#include <bits/stdc++.h>
using namespace std;
void moveZeroes(vector<int> &nums)
{
    int j = -1;
    int n = nums.size();
    for (int i = 0; i < n; i++)
    {
        if (nums[i] == 0)
        {
            j = i;
            break;
        }
    }
    if (j == -1)
        return;
    for (int i = j + 1; i < n; i++)
    {
        if (nums[i] != 0 && nums[j] == 0)
        {
            swap(nums[j], nums[i]);
            j++;
        }
    }
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    moveZeroes(arr1);
    for (int i = 0; i < arr1.size(); i++)
    {
        cout << arr1[i] << " ";
    }
    return 0;
}
