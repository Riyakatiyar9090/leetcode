#include <bits/stdc++.h>
using namespace std;
int largest(vector<int> &arr)
{
    // code here
    // first method
    int n = arr.size();
    int maxi = arr[0];
    for (int i = 0; i < n; i++)
    {
        if (maxi < arr[i])
        {
            maxi = arr[i];
        }
    }
    return maxi;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int ans = largest(arr);
    cout << ans << endl;
    return 0;
}
