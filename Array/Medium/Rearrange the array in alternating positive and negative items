#include <bits/stdc++.h>
using namespace std;
vector<int> rearrangeArray(vector<int> &nums)
{
    int n = nums.size();
    vector<int> ans(n);
    int x = 0, y = 1;
    for (int i = 0; i < n; i++)
    {
        if (nums[i] < 0)
        {
            ans[y] = nums[i];
            y = y + 2;
        }
        else
        {
            ans[x] = nums[i];
            x += 2;
        }
    }
    return ans;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    // vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    // int k;
    // cin >> k;
    vector<int> ans = rearrangeArray(arr1);
    // cout << ans << endl;
    for (int i = 0; i < n; i++)
    {
        cout << ans[i] << " ";
    }

    return 0;
}
