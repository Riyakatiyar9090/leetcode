#include <bits/stdc++.h>
using namespace std;
int maxSubarraySum(vector<int> &arr)
{
    // Code here
    int sum = 0;
    int maxi = INT_MIN;
    for (int i = 0; i < arr.size(); i++)
    {
        sum = sum + arr[i];
        maxi = max(maxi, sum);
        if (sum < 0)
        {
            sum = 0;
        }
    }
    return maxi;
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    // vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    // int k;
    // cin >> k;
    int ans = maxSubarraySum(arr1);
    cout << ans << endl;
    // for (int i = 0; i < n; i++)
    // {
    //     cout << arr1[i] << " ";
    // }

    return 0;
}
