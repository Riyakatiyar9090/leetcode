#include <bits/stdc++.h>
using namespace std;
void nextPermutation(vector<int> &nums)
{
    int index = -1;
    int n = nums.size();
    for (int i = n - 2; i >= 0; i--)
    {
        if (nums[i] < nums[i + 1])
        {
            index = i;
            break;
        }
    }
    if (index == -1)
    {
        reverse(nums.begin(), nums.end());
        return;
    }
    for (int i = n - 1; i >= 0; i--)
    {
        if (nums[i] > nums[index])
        {
            swap(nums[index], nums[i]);
            break;
        }
    }
    sort(nums.begin() + index + 1, nums.end());
}
int main()
{
    int n;
    cin >> n;
    vector<int> arr1(n);
    // vector<int> arr2(n);
    for (int i = 0; i < n; i++)
    {
        cin >> arr1[i];
    }
    // int k;
    // cin >> k;
    nextPermutation(arr1);
    // cout << ans << endl;
    for (int i = 0; i < n; i++)
    {
        cout << arr1[i] << " ";
    }

    return 0;
}
